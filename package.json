{
  "name": "fauna-shell",
  "description": "faunadb shell",
  "version": "0.2.3",
  "author": "Alvaro Videla",
  "bin": {
    "fauna": "./bin/run"
  },
  "bugs": "https://github.com/fauna/fauna-shell/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/plugin-help": "1",
    "cli-table": "^0.3.1",
    "cli-ux": "^4.6.1",
    "faunadb": "^2.1.0",
    "globby": "8",
    "ini": "^1.3.5",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/test": "1",
    "chai": "4",
    "eslint": "4",
    "eslint-config-oclif": "1",
    "mocha": "5",
    "nyc": "11"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/fauna/fauna-shell",
  "keywords": [
    "fauna",
    "faunadb",
    "shell",
    "database",
    "nosql",
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "fauna",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fauna/fauna-shell",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
